        {
            "name": "go Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            //"port": 2345,
            //"host": "127.0.0.1",
            "program": "${fileDirname}", //workspaceRoot
            "env": {},
            "args": [],
            "showLog": true
        },
        {
            "name": "Python38",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceRoot}",
            "console": "internalConsole",
            "stopOnEntry": false, // 设为true时程序将暂停在程序入口处，一般设置为false
        },
        {
            "name": "Python:内置终端",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "program": "${file}",
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            },
            "envFile": "${workspaceFolder}/.env",
            "args": [] // 程序调试时传递给程序的命令行参数，一般为空
        },
        {
            "name": "Python:扩展终端",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "program": "${file}",
            "cwd": "",
            "console": "externalTerminal",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            },
            "envFile": "${workspaceFolder}/.env",
            "args": [
                //"--quiet", "--norepeat"
                "run",
                "--no-debugger",
                "--no-reload"
            ]
        },
    ]
}
